def mod(a, b) -> Int {
    while a >= b {
          a = a - b
    }
    return a
}

# 1 - true, 0 - false
def isprime(x) -> Int {
    if x <= 1 {
       return 0
    }
    if x == 2 {
       return 1
    }
    let i = 2
    while i <= x/2 {
          if mod(x, i) == 0 {
             return 0
          }
          i = i+1
    }
    return 1
}

def main() -> Int {
    return isprime(97)
}


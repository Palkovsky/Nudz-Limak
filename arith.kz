def max(arr: @int, size: int) -> int {
   let i: int = 0
   let max: @int = arr
   while i < size {
     if @(arr+i) > @max {
        max = arr+i
     }
     i = i+1
   } 
   return @max
}

def main() -> int {
    let n: int = 5
    let x: @int = [n]

    @(x+0) = 70
    @(x+1) = 2
    @(x+2) = 10
    @(x+3) = 89
    @(x+4) = 91

    @((x+1)+2) = 100

    return max(x, n)
}